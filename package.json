{
  "name": "githupdates",
  "version": "0.1.0",
  "private": false,
  "homepage": "http://jmahc.github.io/githupdates",
  "dependencies": {
    "fetch": "^1.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "github-api": "3.0.0",
    "prettier-standard": "^5.0.0",
    "react-scripts": "1.0.6"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
